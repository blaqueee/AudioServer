<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views https://axelor.com/xml/ns/object-views/object-views_7.4.xsd">

    <grid title="Log grid" name="job-log-grid" model="com.axelor.apps.audio.db.JobLog"
            canSave="false" canEdit="false" canDelete="false" canNew="false">
        <field name="customOffice"/>
        <field name="status"/>
        <field name="sentAt"/>
    </grid>

    <form title="Log form" name="job-log-form" model="com.axelor.apps.audio.db.JobLog"
            canNew="false" canEdit="false" canSave="false" canDelete="false">
        <panel readonly="true">
            <field name="customOffice"/>
            <field name="status"/>
            <field name="sentAt"/>
        </panel>
    </form>

</object-views>