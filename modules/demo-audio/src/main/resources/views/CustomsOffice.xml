              xsi:schemaLocation="http://axelor.com/xml/ns/object-views https://axelor.com/xml/ns/object-views/object-views_7.4.xsd">

    <grid title="Customs organs" name="audio-customs-office-grid" model="com.axelor.apps.audio.db.CustomsOffice">
        <field name="name"/>
        <field name="code"/>
        <field name="type"/>
        <field name="parent"/>
        <field name="isActive"/>
